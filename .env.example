# Yalidine API Credentials
YALIDINE_API_ID=your_yalidine_api_id
YALIDINE_API_TOKEN=your_yalidine_api_token

# Database Configuration - Railway
DATABASE_URL=${{ MySQL-gWyp.MYSQL_URL }}
DATABASE_HOST=${{ MySQL-gWyp.MYSQL_HOST }}
DATABASE_USER=${{ MySQL-gWyp.MYSQL_USER }}
DATABASE_PASSWORD=${{ MySQL-gWyp.MYSQL_PASSWORD }}
DATABASE_NAME=${{ MySQL-gWyp.MYSQL_DATABASE }}
DATABASE_PORT=${{ MySQL-gWyp.MYSQL_PORT }}
DATABASE_SSL=true
DATABASE_SSL_REJECT_UNAUTHORIZED=false

# Server Configuration
PORT=5000
NODE_ENV=production
